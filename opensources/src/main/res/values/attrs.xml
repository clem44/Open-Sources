<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CollapsingLayout">
        <attr name="handle" format="reference" />
        <attr name="content" format="reference" />
        <attr name="contentContainer" format="reference" />
        <attr name="titleHeader" format="reference" />
        <attr name="animationDuration" format="integer"/>
        <attr name="expand" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SwellCheckBox">
        <attr name="unCheckColor" format="color"/>
        <attr name="checkedColor" format="color"/>
        <attr name="lineColor" format="color"/>
        <attr name="hideTick" format="boolean"/>
        <attr name="circleRadius" format="integer"/>
        <attr name="style" format="enum">
            <enum name="circle" value="0"/>
            <enum name="square" value="1"/>
            <enum name="tick" value="2"/>
        </attr>
        <attr name="lineWidth" format="dimension"/>
        <attr name="animDuration" format="integer"/>
    </declare-styleable>
</resources>